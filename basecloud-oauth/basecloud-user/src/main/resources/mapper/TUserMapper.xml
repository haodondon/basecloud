<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.example.mapper.TUserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.example.vo.TUserVo">
        <id column="id" property="id" />
        <result column="t_user_name" property="tUserName" />
        <result column="t_user_account" property="tUserAccount" />
        <result column="t_user_password" property="tUserPassword" />
        <result column="t_user_pic" property="tUserPic" />
        <result column="t_user_frozen" property="tUserFrozen" />
        <result column="create_time" property="createTime" />
        <result column="status" property="status" />
    </resultMap>

</mapper>
